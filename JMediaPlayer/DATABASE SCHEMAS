MUSIC:		NAME 		VARCHAR(250)			- NAME OF THIS SONG
			ARTIST 		VARCHAR(250)			- NAME OF THE ARTIST THAT CREATED THIS SONG
			ALBUM		VARCHAR(100)			- NAME OF THE ALBUM ON WHICH THIS SONG RESIDES
			LENGTH		BIGINT					- LENGTH OF THIS SONG( 'HH:MM:SS' ) in seconds
			GENRE 		VARCHAR(50)				- GENRE OF THIS SONG
			FILELOC		VARCHAR(500)	PK		- LOCATION OF THIS FILE ON DISK

VIDEO
			NAME		VARCHAR(250)			- NAME OF THIS VIDEO
			LENGTH		BIGINT					- LENGTH OF THIS VIDEO( 'HH:MM:SS' )in seconds
			GENRE		VARCHAR(50)				- GENRE OF THIS VIDEO
			FILELOC		VARCHAR(500)	PK		- LOCATION OF THIS FILE ON DISK
			
PLAYLIST			
		    PLNAME		VARCHAR(250)	PK		- NAME OF THIS PLAYLIST
		    TYPE		VARCHAR(25)				- TYPE OF PLAYIST THIS IS(MUSIC, VIDEO)
		    
MUSICPL		PLNAME		VARCHAR(250)	FK		- NAME OF A PLAYLIST
			SONG		VARCHAR(250)			- NAME OF CONTENT ITEM WITHIN THIS PLAYLIST
			FILELOC		VARCHAR(500)	FK		- LOCATION OF CONTENT ON DISK

VIDEOPL		PLNAME		VARCHAR(250)	FK		- NAME OF A PLAYLIST
			VIDEO		VARCHAR(250)			- NAME OF CONTENT ITEM WITHIN THIS PLAYLIST
			FILELOC		VARCHAR(500)	FK		- LOCATION OF CONTENT ON DISK
			
SERVER_PUNISHMENTS		IP_ADDRESS		VARCHAR(150)
						USERNAME		VARCHAR(30)
						PUNISHMENT		VARCHAR(4)